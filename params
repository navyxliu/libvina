#test parameters for Matrix-multiplication
TEST_INFO=-DMM_TEST_TYPE=float -DMM_TEST_GRANULARITY=256 -DMM_TEST_SIZE_N=2048 -DMM_TEST_K=8

#test parameters for Vector manipulation
TEST_INFO+=-DVEC_TEST_TYPE=float -DVEC_TEST_GRANULARITY=20000000 -DVEC_TEST_SIZE_N=80000000 -DVEC_TEST_K=4

#parameters for Image processing
TEST_INFO+=-DIMG_TEST_SIZE_M=256 -DIMG_TEST_SIZE_N=256 -DIMG_TEST_GRANULARITY=128

